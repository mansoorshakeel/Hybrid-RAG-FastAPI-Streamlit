    "from sentence_transformers import CrossEncoder\n",
    "\n",
    "reranker = CrossEncoder(\"cross-encoder/ms-marco-MiniLM-L-6-v2\")\n",
    "\n",
    "def rerank(question, docs, top_n=4):\n",
    "    pairs  = [(question, d.page_content) for d in docs]          # inputs for reranker\n",
    "    scores = reranker.predict(pairs)                              # relevance scores\n",
    "    ranked = sorted(zip(docs, scores), key=lambda x: x[1], reverse=True)  # sort by score\n",
    "    return ranked[:top_n]     \n",
    " \n",
    "ranked = rerank(query, docs, top_n=4)                             # get top 4 (doc, score)\n",
    "for i, (d, s) in enumerate(ranked, 1):\n",
    "    print(i, \"score:\", float(s), \"|\", d.page_content[:200])\n",
    "                                   # return (doc, score)\n",
    "\n"
