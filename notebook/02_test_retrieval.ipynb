    "import os\n",
    "from pinecone.grpc import PineconeGRPC as Pinecone\n",
    "from langchain_huggingface import HuggingFaceEmbeddings\n",
    "from langchain_community.retrievers import PineconeHybridSearchRetriever\n",
    "from pinecone_text.sparse import BM25Encoder\n",
    "\n",
    "\n",
    "\n",
    "# 1) Dense embeddings\n",
    "embeddings = HuggingFaceEmbeddings(model_name=\"sentence-transformers/all-MiniLM-L6-v2\")\n",
    "\n",
    "# 2) Pinecone index\n",
    "pc = Pinecone(api_key=os.environ[\"PINECONE_API_KEY\"].strip())\n",
    "PINECONE_INDEX = os.getenv(\"PINECONE_INDEX\")   # read env var into a Python variable\n",
    "index = pc.Index(PINECONE_INDEX)    \n",
    "\n",
    "# 3) Load same BM25 encoder values created during ingestion\n",
    "bm25 = BM25Encoder().load(\"bm25_values.json\")\n",
    "\n",
    "# 4) Hybrid retriever\n",
    "retriever = PineconeHybridSearchRetriever(\n",
    "    embeddings=embeddings,\n",
    "    sparse_encoder=bm25,\n",
    "    index=index,\n",
    "    top_k=10,\n",
    "    alpha=0.75  # ✅ change: 0.2 keyword-heavy, 0.8 semantic-heavy\n",
    ")\n",
    "\n",
    "# 5) Query\n",
    "query = \"What is the difference between TCP and UDP\"\n",
    "docs = retriever.invoke(query)\n",
    "\n",
    "for i, d in enumerate(docs, 1):\n",
    "    print(\"----\", i, \"----\")\n",
    "    print(d.page_content[:400])\n",
    "    print(d.metadata)\n",
    "print(\"Returned docs:\", len(docs))"
   ]
